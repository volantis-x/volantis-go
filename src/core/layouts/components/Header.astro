---
// import Marquee from "../../../../content/components/Marquee.config";
import BaseTopMarquee from "@layoutComps/header/base/TopMarquee.astro";
import BaseTopNav from "@layoutComps/header/base/TopNav.astro";
import BaseHeaderContent from "@layoutComps/header/base/HeaderContent.astro";
import { getProcessedConfig } from "../../../core/bootstrap/store";
const Marquee = getProcessedConfig("marquee");
---

{
  Marquee?.enable && (
    <>
      {{
        base: <BaseTopMarquee />,
      }[__VOLANTIS_THEME__] || <BaseTopMarquee />}
    </>
  )
}

{
  {
    base: (
      <>
        <BaseTopNav />
        <BaseHeaderContent />
      </>
    ),
  }[__VOLANTIS_THEME__] || (
    <>
      <BaseTopNav />
      <BaseHeaderContent />
    </>
  )
}
