{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["./src/layouts/components/*"],
      "@/lib/*": ["./src/helpers/lib/*"],
      "@/utils/*": ["./src/helpers/utils/*"],
      "@/types/*": ["./src/types/*"],
      "@/*": ["./src/*"]
    },
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ]
  }
}
